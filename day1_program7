Value Performance
#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
    
    int a;
    scanf("%d",&a);
    float b;
    float final = ceil(b);
    scanf("%f",&b);
    printf("%d\n",a);
    printf("%.2f",b,final);
    return 0;
}
